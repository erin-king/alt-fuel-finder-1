<h1> The results for FACADE SERACHED ZIP HERE LATER... </h1>
<section class='total-results'>
  <h3> <%= facade.total_results %> </h3>
</section>

<section>
  <% facade.results.each do |result| %>
  <ul class='results-list'>
    <li>Name: <%= result.name %></li>
    <li>Address: <%= result.address %></li>
    <li>Fuel Type: <%= result.fuel_type %></li>
    <li>Distance: <%= result.distance %></li>
    <li>Access Times: <%= result.access_times %></li>
  </ul>
  <% end %>
</section>
